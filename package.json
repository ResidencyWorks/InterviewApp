{
  "name": "interview-app",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered interview preparation application",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check:all": "biome check . && biome format --write . && tsc --noEmit",
    "fix:all": "biome check --write . && biome format --write . && tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "verify-setup": "bash .devcontainer/scripts/verify-setup.sh",
    "setup": "bash .devcontainer/scripts/setup-dev.sh",
    "install-deps": "bash .devcontainer/scripts/install-deps.sh",
    "prepare": "lefthook install",
    "clean": "rm -rf .next dist build coverage test-results",
    "clean:all": "rm -rf node_modules .next dist build coverage test-results && pnpm install"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^10.20.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@upstash/redis": "^1.35.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.546.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.6",
    "openai": "^6.6.0",
    "posthog-js": "^1.277.0",
    "posthog-node": "^5.10.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^23.0.0",
    "lefthook": "^1.5.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "zod": "^4.1.12"
  },
  "keywords": [
    "interview",
    "ai",
    "preparation",
    "nextjs",
    "typescript",
    "react"
  ],
  "author": "InterviewApp Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/interview-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/interview-app/issues"
  },
  "homepage": "https://github.com/your-org/interview-app#readme"
}
