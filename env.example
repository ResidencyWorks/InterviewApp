# Environment variables for AI/ASR Evaluation Feature
# Copy to .env.local and fill in real secrets.

# OpenAI API key for Whisper + GPT evaluation
OPENAI_API_KEY=your_openai_key_here

# PostHog project API key (server-side)
POSTHOG_API_KEY=your_posthog_key_here

# Supabase project URL and service role key
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Redis / Upstash URL for BullMQ queue
UPSTASH_REDIS_REST_URL=redis://username:password@host:port

# Request timeout (ms) for synchronous evaluation attempt
REQUEST_TIMEOUT_MS=30000
# Sync evaluation maximum wait (ms) before returning 202
SYNC_TIMEOUT_MS=30000

# Rate limit: requests per minute per API key
RATE_LIMIT_RPM=60

# Optional webhook delivery target & secret
EVALUATION_WEBHOOK_URL=
EVALUATION_WEBHOOK_SECRET=

# Node environment
NODE_ENV=development
